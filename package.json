{
  "name": "electron-music-player",
  "version": "1.0.0",
  "description": "Music Player Desktop Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "electron",
    "music",
    "player"
  ],
  "author": "Your Team",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.1.0",
    "electron-builder": "^24.6.4"
  },
  "build": {
    "appId": "com.yourteam.musicplayer",
    "productName": "Music Player",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}